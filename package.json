  },
  "scripts": {
    "build": "yarn w/core build && yarn w/mongoose build && yarn w/server build && yarn w/example build",
    "lint": "eslint --ext .ts,.tsx packages",
    "prepare": "yarn build",
    "test": "yarn w/core test && yarn w/server test && yarn w/mongoose test",
    "typecheck": "tsc -p tsconfig.eslint.json",
    "w/core": "yarn workspace @sheeted/core",
    "w/example": "yarn workspace sheeted-examples",
    "w/mongoose": "yarn workspace @sheeted/mongoose",
    "w/server": "yarn workspace @sheeted/server",
    "w/ui": "yarn workspace @sheeted/ui"
  },
  "workspaces": [
    "packages/*",
    "examples"
  ]
